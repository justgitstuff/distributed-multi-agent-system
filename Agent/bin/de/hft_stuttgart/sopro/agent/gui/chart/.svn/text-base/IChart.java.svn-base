package de.hft_stuttgart.sopro.agent.gui.chart;

public interface IChart {

	/**
	 * This method creates the inital state of the chart.
	 */
	public void create();

	/**
	 * This method computes the value which the chart will show for the next
	 * negotiation round.
	 */
	public void computeNextRound();

	/**
	 * This method updates the chart.
	 */
	public void update();

	/**
	 * This method can be used do something before closing the chart, e.g. free
	 * resources
	 */
	public void dispose();
}
